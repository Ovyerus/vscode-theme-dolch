{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dolch Light",
	"colors": {
		"focusBorder": "#00a3b7",
		"foreground": "#555c5b",
		"errorForeground": "#b46b64",
		"editor.background": "#f3fbfa",
		"editor.foreground": "#555c5b",
		"editor.lineHighlightBackground": "#ebf4f2",
		"editorCursor.foreground": "#004b6c",
		"editorLineNumber.foreground": "#a1a9a8",
		"editorActiveLineNumber.foreground": "#212726",
		"editor.selectionBackground": "#005d7a40",
		"editor.selectionHighlightBackground": "#aaedf7ff",
		"editor.inactiveSelectionBackground": "#3b424140",
		"editorError.foreground": "#95413c",
		"editorWarning.foreground": "#6c5913",
		"editorInfo.foreground": "#005f9c",
		"editorHint.foreground": "#005f9c",
		"notification.errorBackground": "#d39690",
		"notification.errorForeground": "#95413c",
		"notification.infoBackground": "#89a7d2",
		"notification.infoForeground": "#005f9c",
		"notification.warningBackground": "#b6a478",
		"notification.warningForeground": "#6c5913",
		"debugToolBar.background": "#dde6e5ff",
		"editorWidget.background": "#dde6e5ff",
		"editorSuggestWidget.background": "#f3fbfaff",
		"editorGroup.border": "#ccd4d3",
		"editorGroupHeader.noTabsBackground": "#f3fbfa",
		"editorGroupHeader.tabsBackground": "#dde6e5",
		"tab.border": "#ccd4d3",
		"tab.inactiveBackground": "#dde6e5",
		"tab.inactiveForeground": "#707776",
		"tab.activeBackground": "#f3fbfa",
		"tab.activeForeground": "#212726",
		"peekViewEditor.background": "#ccd4d355",
		"peekViewTitle.background": "#f3fbfaff",
		"peekView.border": "#ccd4d3",
		"scrollbar.shadow": "#2127261a",
		"scrollbarSlider.background": "#21272613",
		"scrollbarSlider.activeBackground": "#21272626",
		"scrollbarSlider.hoverBackground": "#21272626",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#83c4cc",
		"editorGutter.addedBackground": "#9cc2a5",
		"editorGutter.deletedBackground": "#e1aca6",
		"editorOverviewRuler.modifiedForeground": "#83c4cc",
		"editorOverviewRuler.addedForeground": "#9cc2a5",
		"editorOverviewRuler.deletedForeground": "#e1aca6",
		"editorOverviewRuler.infoForeground": "#6f94c5",
		"editorOverviewRuler.warningForeground": "#a3905f",
		"editorOverviewRuler.errorForeground": "#c4817a",
		"diffEditor.removedTextBackground": "#c4817a26",
		"diffEditor.insertedTextBackground": "#6b9d781a",
		"sideBarTitle.foreground": "#212726",
		"sideBar.background": "#ebf4f2",
		"sideBarSectionHeader.background": "#ccd4d3",
		"list.highlightForeground": "#0092a7",
		"list.hoverBackground": "#ccd4d3",
		"list.inactiveSelectionBackground": "#a1a9a84d",
		"list.activeSelectionBackground": "#00a3b740",
		"list.focusBackground": "#00a3b740",
		"list.inactiveSelectionForeground": "#212726",
		"list.activeSelectionForeground": "#212726",
		"list.focusForeground": "#212726",
		"list.errorForeground": "#95413c",
		"list.warningForeground": "#6c5913",
		"gitDecoration.modifiedResourceForeground": "#006a7b",
		"gitDecoration.addedResourceForeground": "#1a6839",
		"gitDecoration.untrackedResourceForeground": "#1a6839",
		"gitDecoration.deletedResourceForeground": "#95413c",
		"gitDecoration.conflictingResourceForeground": "#95413c",
		"dropdown.background": "#dde6e5",
		"dropdown.border": "#ccd4d3",
		"dropdown.foreground": "#555c5b",
		"input.background": "#f3fbfa",
		"input.border": "#ccd4d3",
		"input.foreground": "#555c5b",
		"input.placeholderForeground": "#a1a9a8",
		"button.background": "#23b6c6",
		"button.foreground": "#f3fbfa",
		"button.hoverBackground": "#5ec8d6",
		"badge.background": "#0092a7",
		"badge.foreground": "#f3fbfa",
		"activityBarBadge.background": "#0092a7",
		"activityBarBadge.foreground": "#f3fbfa",
		"activityBar.background": "#ccd4d3",
		"activityBar.foreground": "#3b4241",
		"statusBar.background": "#dde6e5",
		"statusBar.foreground": "#555c5b",
		"statusBar.noFolderBackground": "#dde6e5",
		"statusBar.noFolderForeground": "#555c5b",
		"statusBar.debuggingBackground": "#dde6e5",
		"statusBar.debuggingForeground": "#555c5b",
		"panel.border": "#ccd4d3",
		"panelTitle.activeBorder": "#00a3b7",
		"titleBar.activeBackground": "#555c5b",
		"titleBar.activeForeground": "#ebf4f2",
		"titleBar.inactiveBackground": "#707776",
		"titleBar.inactiveForeground": "#dde6e5",
		"breadcrumb.foreground": "#899190",
		"breadcrumb.focusForeground": "#008098",
		"breadcrumb.activeSelectionForeground": "#008098",
		"breadcrumbPicker.background": "#ebf4f2"
	},
	"tokenColors": [
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#555c5b"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#555c5b"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#899190"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#004b6c"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#a1a9a8"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#212726"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008098"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#005d7a"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name.function",
				"entity.name.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#3b4241"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#3b4241"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.type",
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#3b4241"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#a55650"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#a55650"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#3b4241"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008098"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#3b4241"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#3b4241"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#3b4241"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#3b4241"
			}
		}
	]
}