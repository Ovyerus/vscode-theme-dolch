		"notification.infoBackground": "#72ABF3",
		"notification.infoForeground": "#006ABB",
		"notification.warningBackground": "#BEA75C",
		"notification.warningForeground": "#776500",
		"debugToolBar.background": "#CDD1D3FF",
		"editorWidget.background": "#CDD1D3FF",
		"editorSuggestWidget.background": "#F0F4F7FF",
		"editorGroup.border": "#E0E5E7",
		"editorGroupHeader.noTabsBackground": "#F0F4F7",
		"editorGroupHeader.tabsBackground": "#F0F4F7",
		"tab.border": "#F0F4F7",
		"tab.inactiveBackground": "#F0F4F7",
		"tab.inactiveForeground": "#5B5F61",
		"tab.activeBackground": "#F0F4F7",
		"tab.activeForeground": "#191C1E",
		"tab.hoverBackground": "#E0E5E7",
		"tab.hoverBorder": "#48A3BB",
		"tab.activeBorder": "#007391",
		"peekView.border": "#CDD1D3",
		"peekViewTitle.background": "#E0E5E7FF",
		"peekViewEditor.background": "#B7BBBE2B",
		"scrollbar.shadow": "#0000001A",
		"scrollbarSlider.background": "#191C1E13",
		"scrollbarSlider.activeBackground": "#191C1E26",
		"scrollbarSlider.hoverBackground": "#191C1E26",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#00BBCF",
		"editorGutter.addedBackground": "#68B984",
		"editorGutter.deletedBackground": "#EF8E82",
		"editorOverviewRuler.modifiedForeground": "#00597433",
		"editorOverviewRuler.addedForeground": "#00597433",
		"editorOverviewRuler.deletedForeground": "#00597433",
		"editorOverviewRuler.infoForeground": "#599AE5E6",
		"editorOverviewRuler.warningForeground": "#AC9647E6",
		"editorOverviewRuler.errorForeground": "#DF7B70E6",
		"editorOverviewRuler.findMatchForeground": "#CC8853E6",
		"editorOverviewRuler.bracketMatchForeground": "#191C1E4D",
		"editorOverviewRuler.wordHighlightForeground": "#191C1E4D",
		"editorOverviewRuler.wordHighlightStrongForeground": "#191C1E4D",
		"diffEditor.removedTextBackground": "#DF7B7026",
		"diffEditor.insertedTextBackground": "#51A8711A",
		"sideBarTitle.foreground": "#191C1E",
		"sideBar.background": "#E0E5E7",
		"sideBarSectionHeader.background": "#B7BBBE",
		"list.highlightForeground": "#1D92AD",
		"list.hoverBackground": "#B7BBBE",
		"list.inactiveSelectionBackground": "#898D904D",
		"list.activeSelectionBackground": "#48A3BB40",
		"list.focusBackground": "#48A3BB40",
		"list.inactiveSelectionForeground": "#191C1E",
		"list.activeSelectionForeground": "#191C1E",
		"list.focusForeground": "#191C1E",
		"list.errorForeground": "#AD423B",
		"list.warningForeground": "#776500",
		"gitDecoration.modifiedResourceForeground": "#007991",
		"gitDecoration.addedResourceForeground": "#00763C",
		"gitDecoration.untrackedResourceForeground": "#00763C",
		"gitDecoration.deletedResourceForeground": "#AD423B",
		"gitDecoration.conflictingResourceForeground": "#AD423B",
		"dropdown.background": "#E0E5E7",
		"dropdown.border": "#B7BBBE",
		"dropdown.foreground": "#44484A",
		"input.background": "#F0F4F7",
		"input.border": "#B7BBBE",
		"input.foreground": "#44484A",
		"input.placeholderForeground": "#898D90",
		"button.background": "#66B3CA",
		"button.foreground": "#F0F4F7",
		"button.hoverBackground": "#82C4D8",
		"badge.background": "#1D92AD",
		"badge.foreground": "#F0F4F7",
		"activityBarBadge.background": "#1D92AD",
		"activityBarBadge.foreground": "#F0F4F7",
		"activityBar.background": "#B7BBBE",
		"activityBar.foreground": "#2E3133",
		"statusBar.background": "#CDD1D3",
		"statusBar.foreground": "#44484A",
		"statusBar.noFolderBackground": "#CDD1D3",
		"statusBar.noFolderForeground": "#44484A",
		"statusBar.debuggingBackground": "#CDD1D3",
		"statusBar.debuggingForeground": "#44484A",
		"panel.border": "#B7BBBE",
		"panelTitle.activeBorder": "#48A3BB",
		"titleBar.activeBackground": "#44484A",
		"titleBar.activeForeground": "#E0E5E7",
		"titleBar.inactiveBackground": "#5B5F61",
		"titleBar.inactiveForeground": "#CDD1D3",
		"breadcrumb.foreground": "#898D90",
		"breadcrumb.focusForeground": "#00829F",
		"breadcrumb.activeSelectionForeground": "#00829F",
		"breadcrumbPicker.background": "#E0E5E7"
	},
	"tokenColors": [
		{
			"name": "Identifier",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"variable.other.readwrite",
				"variable.other.constant",
				"variable.other.readonly"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#44484A"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#44484A"
			}
		},
		{
			"name": "Member Access",
			"scope": [
				"variable.other.property"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#5B5F61"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#44484A"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#898D90"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#005476"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#898D90"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#191C1E"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00829F"
			}
		},
		{
			"scope": [
				"meta.template.expression.ts"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#006383"
			}
		},
		{
			"name": "Type name",
			"scope": [
				"entity.name.type",
				"support.type"
			],
			"settings": {
				"foreground": "#007574"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name",
				"entity.name.class",
				"entity.name.function",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#2E3133"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#191C1E"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#191C1E"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#2E3133"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.class",
				"support.function",
				"support.constant",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#2E3133"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#BE554C"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#BE554C"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#2E3133"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#191C1E"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00829F"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#2E3133"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#2E3133"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#2E3133"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#191C1E"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#2E3133"
			}
		}
	],
	"semanticHighlighting": true
}