{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dolch Light",
	"colors": {
		"focusBorder": "#54A6B1",
		"foreground": "#555C5B",
		"errorForeground": "#BE756B",
		"editor.background": "#F3FBFA",
		"editor.foreground": "#555C5B",
		"editor.lineHighlightBackground": "#EBF4F2",
		"editorCursor.foreground": "#005464",
		"editorLineNumber.foreground": "#A1A9A8",
		"editorActiveLineNumber.foreground": "#212726",
		"editor.selectionBackground": "#00637240",
		"editor.selectionHighlightBackground": "#B9ECF4FF",
		"editor.inactiveSelectionBackground": "#3B424140",
		"editorError.foreground": "#A24D45",
		"editorWarning.foreground": "#77661F",
		"editorInfo.foreground": "#2369A9",
		"editorHint.foreground": "#2369A9",
		"notification.errorBackground": "#D99D94",
		"notification.errorForeground": "#A24D45",
		"notification.infoBackground": "#92ADD9",
		"notification.infoForeground": "#2369A9",
		"notification.warningBackground": "#BBAB7E",
		"notification.warningForeground": "#77661F",
		"debugToolBar.background": "#DDE6E5FF",
		"editorWidget.background": "#DDE6E5FF",
		"editorSuggestWidget.background": "#F3FBFAFF",
		"editorGroup.border": "#CCD4D3",
		"editorGroupHeader.noTabsBackground": "#F3FBFA",
		"editorGroupHeader.tabsBackground": "#DDE6E5",
		"tab.border": "#CCD4D3",
		"tab.inactiveBackground": "#DDE6E5",
		"tab.inactiveForeground": "#707776",
		"tab.activeBackground": "#F3FBFA",
		"tab.activeForeground": "#212726",
		"peekViewEditor.background": "#CCD4D355",
		"peekViewTitle.background": "#F3FBFAFF",
		"peekView.border": "#CCD4D3",
		"scrollbar.shadow": "#2127261A",
		"scrollbarSlider.background": "#21272613",
		"scrollbarSlider.activeBackground": "#21272626",
		"scrollbarSlider.hoverBackground": "#21272626",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#88C8D2",
		"editorGutter.addedBackground": "#A0C7AB",
		"editorGutter.deletedBackground": "#E6B1AA",
		"editorOverviewRuler.modifiedForeground": "#88C8D2",
		"editorOverviewRuler.addedForeground": "#A0C7AB",
		"editorOverviewRuler.deletedForeground": "#E6B1AA",
		"editorOverviewRuler.infoForeground": "#7A9CCD",
		"editorOverviewRuler.warningForeground": "#AA9966",
		"editorOverviewRuler.errorForeground": "#CC8980",
		"diffEditor.removedTextBackground": "#CC898026",
		"diffEditor.insertedTextBackground": "#70A6811A",
		"sideBarTitle.foreground": "#212726",
		"sideBar.background": "#EBF4F2",
		"sideBarSectionHeader.background": "#CCD4D3",
		"list.highlightForeground": "#3695A1",
		"list.hoverBackground": "#CCD4D3",
		"list.inactiveSelectionBackground": "#A1A9A84D",
		"list.activeSelectionBackground": "#54A6B140",
		"list.focusBackground": "#54A6B140",
		"list.inactiveSelectionForeground": "#212726",
		"list.activeSelectionForeground": "#212726",
		"list.focusForeground": "#212726",
		"list.errorForeground": "#A24D45",
		"list.warningForeground": "#77661F",
		"gitDecoration.modifiedResourceForeground": "#007689",
		"gitDecoration.addedResourceForeground": "#247546",
		"gitDecoration.untrackedResourceForeground": "#247546",
		"gitDecoration.deletedResourceForeground": "#A24D45",
		"gitDecoration.conflictingResourceForeground": "#A24D45",
		"dropdown.background": "#DDE6E5",
		"dropdown.border": "#CCD4D3",
		"dropdown.foreground": "#555C5B",
		"input.background": "#F3FBFA",
		"input.border": "#CCD4D3",
		"input.foreground": "#555C5B",
		"input.placeholderForeground": "#A1A9A8",
		"button.background": "#6FB7C1",
		"button.foreground": "#F3FBFA",
		"button.hoverBackground": "#88C8D2",
		"badge.background": "#3695A1",
		"badge.foreground": "#F3FBFA",
		"activityBarBadge.background": "#3695A1",
		"activityBarBadge.foreground": "#F3FBFA",
		"activityBar.background": "#CCD4D3",
		"activityBar.foreground": "#3B4241",
		"statusBar.background": "#DDE6E5",
		"statusBar.foreground": "#555C5B",
		"statusBar.noFolderBackground": "#DDE6E5",
		"statusBar.noFolderForeground": "#555C5B",
		"statusBar.debuggingBackground": "#DDE6E5",
		"statusBar.debuggingForeground": "#555C5B",
		"panel.border": "#CCD4D3",
		"panelTitle.activeBorder": "#54A6B1",
		"titleBar.activeBackground": "#555C5B",
		"titleBar.activeForeground": "#EBF4F2",
		"titleBar.inactiveBackground": "#707776",
		"titleBar.inactiveForeground": "#DDE6E5",
		"breadcrumb.foreground": "#899190",
		"breadcrumb.focusForeground": "#008491",
		"breadcrumb.activeSelectionForeground": "#008491",
		"breadcrumbPicker.background": "#EBF4F2"
	},
	"tokenColors": [
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#555C5B"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#555C5B"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#899190"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#005464"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#A1A9A8"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#212726"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008491"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#006372"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name.function",
				"entity.name.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#3B4241"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#3B4241"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.type",
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#3B4241"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#B06158"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#B06158"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#3B4241"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008491"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#3B4241"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#3B4241"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#3B4241"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212726"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#3B4241"
			}
		}
	]
}