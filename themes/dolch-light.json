{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dolch Light",
	"colors": {
		"focusBorder": "#47A7B4",
		"foreground": "#575C5B",
		"errorForeground": "#D16A60",
		"editor.background": "#F5FAFA",
		"editor.foreground": "#575C5B",
		"editor.lineHighlightBackground": "#EDF3F2",
		"editorCursor.foreground": "#005568",
		"editorLineNumber.foreground": "#A3A9A8",
		"editorCodeLens.foreground": "#A3A9A8",
		"editorActiveLineNumber.foreground": "#222726",
		"editor.selectionBackground": "#00859560",
		"editor.inactiveSelectionBackground": "#72767660",
		"editor.selectionHighlightBackground": "#66B8C450",
		"editor.wordHighlightBackground": "#A3A9A850",
		"editor.wordHighlightStrongBackground": "#47A7B450",
		"editor.findMatchBackground": "#88440060",
		"editor.findMatchHighlightBackground": "#88440030",
		"editor.findRangeHighlightBackground": "#BABFBF50",
		"editorLink.activeForeground": "#005568",
		"editorBracketMatch.background": "#BABFBF50",
		"editorBracketMatch.border": "#BABFBFBF",
		"editorError.foreground": "#AF433C",
		"editorWarning.foreground": "#786600",
		"editorInfo.foreground": "#006BBC",
		"editorHint.foreground": "#006BBC",
		"notification.errorBackground": "#F29186",
		"notification.errorForeground": "#AF433C",
		"notification.infoBackground": "#76AFF7",
		"notification.infoForeground": "#006BBC",
		"notification.warningBackground": "#C1AA5F",
		"notification.warningForeground": "#786600",
		"debugToolBar.background": "#E0E5E4FF",
		"editorWidget.background": "#E0E5E4FF",
		"editorSuggestWidget.background": "#F5FAFAFF",
		"editorGroup.border": "#EDF3F2",
		"editorGroupHeader.noTabsBackground": "#F5FAFA",
		"editorGroupHeader.tabsBackground": "#F5FAFA",
		"tab.border": "#F5FAFA",
		"tab.inactiveBackground": "#F5FAFA",
		"tab.inactiveForeground": "#727676",
		"tab.activeBackground": "#F5FAFA",
		"tab.activeForeground": "#222726",
		"tab.hoverBackground": "#EDF3F2",
		"tab.hoverBorder": "#47A7B4",
		"tab.activeBorder": "#007586",
		"peekView.border": "#E0E5E4",
		"peekViewTitle.background": "#EDF3F2FF",
		"peekViewEditor.background": "#CED4D32B",
		"scrollbar.shadow": "#0000001A",
		"scrollbarSlider.background": "#22272613",
		"scrollbarSlider.activeBackground": "#22272626",
		"scrollbarSlider.hoverBackground": "#22272626",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#00BED2",
		"editorGutter.addedBackground": "#6BBC87",
		"editorGutter.deletedBackground": "#F29186",
		"editorOverviewRuler.modifiedForeground": "#5CE2F4",
		"editorOverviewRuler.addedForeground": "#5CE2F4",
		"editorOverviewRuler.deletedForeground": "#5CE2F4",
		"editorOverviewRuler.infoForeground": "#5C9DE8",
		"editorOverviewRuler.warningForeground": "#AF9849",
		"editorOverviewRuler.errorForeground": "#E27E72",
		"editorOverviewRuler.wordHighlightForeground": "#A3A9A8",
		"editorOverviewRuler.wordHighlightStrongForeground": "#A3A9A8",
		"diffEditor.removedTextBackground": "#E27E7226",
		"diffEditor.insertedTextBackground": "#54AB741A",
		"sideBarTitle.foreground": "#222726",
		"sideBar.background": "#EDF3F2",
		"sideBarSectionHeader.background": "#CED4D3",
		"list.highlightForeground": "#1A96A4",
		"list.hoverBackground": "#CED4D3",
		"list.inactiveSelectionBackground": "#A3A9A84D",
		"list.activeSelectionBackground": "#47A7B440",
		"list.focusBackground": "#47A7B440",
		"list.inactiveSelectionForeground": "#222726",
		"list.activeSelectionForeground": "#222726",
		"list.focusForeground": "#222726",
		"list.errorForeground": "#AF433C",
		"list.warningForeground": "#786600",
		"gitDecoration.modifiedResourceForeground": "#007A92",
		"gitDecoration.addedResourceForeground": "#00773C",
		"gitDecoration.untrackedResourceForeground": "#00773C",
		"gitDecoration.deletedResourceForeground": "#AF433C",
		"gitDecoration.conflictingResourceForeground": "#AF433C",
		"dropdown.background": "#EDF3F2",
		"dropdown.border": "#CED4D3",
		"dropdown.foreground": "#575C5B",
		"input.background": "#F5FAFA",
		"input.border": "#CED4D3",
		"input.foreground": "#575C5B",
		"input.placeholderForeground": "#A3A9A8",
		"button.background": "#66B8C4",
		"button.foreground": "#F5FAFA",
		"button.hoverBackground": "#82C9D4",
		"badge.background": "#1A96A4",
		"badge.foreground": "#F5FAFA",
		"activityBarBadge.background": "#1A96A4",
		"activityBarBadge.foreground": "#F5FAFA",
		"activityBar.background": "#CED4D3",
		"activityBar.foreground": "#3D4140",
		"statusBar.background": "#E0E5E4",
		"statusBar.foreground": "#575C5B",
		"statusBar.noFolderBackground": "#E0E5E4",
		"statusBar.noFolderForeground": "#575C5B",
		"statusBar.debuggingBackground": "#E0E5E4",
		"statusBar.debuggingForeground": "#575C5B",
		"panel.border": "#CED4D3",
		"panelTitle.activeBorder": "#47A7B4",
		"titleBar.activeBackground": "#575C5B",
		"titleBar.activeForeground": "#EDF3F2",
		"titleBar.inactiveBackground": "#727676",
		"titleBar.inactiveForeground": "#E0E5E4",
		"breadcrumb.foreground": "#A3A9A8",
		"breadcrumb.focusForeground": "#008595",
		"breadcrumb.activeSelectionForeground": "#008595",
		"breadcrumbPicker.background": "#EDF3F2"
	},
	"tokenColors": [
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#575C5B"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#575C5B"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#A3A9A8"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#005568"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#A3A9A8"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#222726"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008595"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#006577"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name.function",
				"entity.name.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#3D4140"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#222726"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#222726"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#3D4140"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.type",
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#3D4140"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#C0574E"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#C0574E"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#3D4140"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#222726"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008595"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#3D4140"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#3D4140"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#3D4140"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#222726"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#3D4140"
			}
		}
	]
}