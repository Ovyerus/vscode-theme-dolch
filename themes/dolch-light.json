		"editorError.foreground": "#AD423B",
		"editorWarning.foreground": "#776500",
		"editorInfo.foreground": "#006ABB",
		"editorHint.foreground": "#006ABB",
		"notification.errorBackground": "#EF8E82",
		"notification.errorForeground": "#AD423B",
		"notification.infoBackground": "#72ABF3",
		"notification.infoForeground": "#006ABB",
		"notification.warningBackground": "#BEA75C",
		"notification.warningForeground": "#776500",
		"debugToolBar.background": "#CFD4D3FF",
		"editorWidget.background": "#CFD4D3FF",
		"editorSuggestWidget.background": "#EFF5F4FF",
		"editorGroup.border": "#E1E7E6",
		"editorGroupHeader.noTabsBackground": "#EFF5F4",
		"editorGroupHeader.tabsBackground": "#EFF5F4",
		"tab.border": "#EFF5F4",
		"tab.inactiveBackground": "#EFF5F4",
		"tab.inactiveForeground": "#5D6261",
		"tab.activeBackground": "#EFF5F4",
		"tab.activeForeground": "#181C1C",
		"tab.hoverBackground": "#E1E7E6",
		"tab.hoverBorder": "#43A4B1",
		"tab.activeBorder": "#007485",
		"peekView.border": "#CFD4D3",
		"peekViewTitle.background": "#E1E7E6FF",
		"peekViewEditor.background": "#BABFBE2B",
		"scrollbar.shadow": "#0000001A",
		"scrollbarSlider.background": "#181C1C13",
		"scrollbarSlider.activeBackground": "#181C1C26",
		"scrollbarSlider.hoverBackground": "#181C1C26",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#00BBCF",
		"editorGutter.addedBackground": "#68B984",
		"editorGutter.deletedBackground": "#EF8E82",
		"editorOverviewRuler.modifiedForeground": "#00597433",
		"editorOverviewRuler.addedForeground": "#00597433",
		"editorOverviewRuler.deletedForeground": "#00597433",
		"editorOverviewRuler.infoForeground": "#599AE5E6",
		"editorOverviewRuler.warningForeground": "#AC9647E6",
		"editorOverviewRuler.errorForeground": "#DF7B70E6",
		"editorOverviewRuler.findMatchForeground": "#CC8853E6",
		"editorOverviewRuler.bracketMatchForeground": "#181C1C4D",
		"editorOverviewRuler.wordHighlightForeground": "#181C1C4D",
		"editorOverviewRuler.wordHighlightStrongForeground": "#181C1C4D",
		"diffEditor.removedTextBackground": "#DF7B7026",
		"diffEditor.insertedTextBackground": "#51A8711A",
		"sideBarTitle.foreground": "#181C1C",
		"sideBar.background": "#E1E7E6",
		"sideBarSectionHeader.background": "#BABFBE",
		"list.highlightForeground": "#1594A2",
		"list.hoverBackground": "#BABFBE",
		"list.inactiveSelectionBackground": "#8C91914D",
		"list.activeSelectionBackground": "#43A4B140",
		"list.focusBackground": "#43A4B140",
		"list.inactiveSelectionForeground": "#181C1C",
		"list.activeSelectionForeground": "#181C1C",
		"list.focusForeground": "#181C1C",
		"list.errorForeground": "#AD423B",
		"list.warningForeground": "#776500",
		"gitDecoration.modifiedResourceForeground": "#007991",
		"gitDecoration.addedResourceForeground": "#00763C",
		"gitDecoration.untrackedResourceForeground": "#00763C",
		"gitDecoration.deletedResourceForeground": "#AD423B",
		"gitDecoration.conflictingResourceForeground": "#AD423B",
		"dropdown.background": "#E1E7E6",
		"dropdown.border": "#BABFBE",
		"dropdown.foreground": "#454A49",
		"input.background": "#EFF5F4",
		"input.border": "#BABFBE",
		"input.foreground": "#454A49",
		"input.placeholderForeground": "#8C9191",
		"button.background": "#63B5C0",
		"button.foreground": "#EFF5F4",
		"button.hoverBackground": "#7EC5D0",
		"badge.background": "#1594A2",
		"badge.foreground": "#EFF5F4",
		"activityBarBadge.background": "#1594A2",
		"activityBarBadge.foreground": "#EFF5F4",
		"activityBar.background": "#BABFBE",
		"activityBar.foreground": "#2E3332",
		"statusBar.background": "#CFD4D3",
		"statusBar.foreground": "#454A49",
		"statusBar.noFolderBackground": "#CFD4D3",
		"statusBar.noFolderForeground": "#454A49",
		"statusBar.debuggingBackground": "#CFD4D3",
		"statusBar.debuggingForeground": "#454A49",
		"panel.border": "#BABFBE",
		"panelTitle.activeBorder": "#43A4B1",
		"titleBar.activeBackground": "#454A49",
		"titleBar.activeForeground": "#E1E7E6",
		"titleBar.inactiveBackground": "#5D6261",
		"titleBar.inactiveForeground": "#CFD4D3",
		"breadcrumb.foreground": "#8C9191",
		"breadcrumb.focusForeground": "#008493",
		"breadcrumb.activeSelectionForeground": "#008493",
		"breadcrumbPicker.background": "#E1E7E6"
	},
	"tokenColors": [
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#454A49"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#454A49"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#8C9191"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#005568"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#8C9191"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#181C1C"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008493"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#006476"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name.function",
				"entity.name.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#2E3332"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#181C1C"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#181C1C"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#2E3332"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.type",
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#2E3332"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#BE554C"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#BE554C"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#2E3332"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#181C1C"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#008493"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#2E3332"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#2E3332"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#2E3332"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#181C1C"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#2E3332"
			}
		}
	]
}