{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dolch",
	"colors": {
		"focusBorder": "#008294",
		"foreground": "#B4B9B8",
		"errorForeground": "#CF695F",
		"editor.background": "#101414",
		"editor.foreground": "#B4B9B8",
		"editor.lineHighlightBackground": "#222625",
		"editorCursor.foreground": "#75F4FF",
		"editorLineNumber.foreground": "#717575",
		"editorCodeLens.foreground": "#717575",
		"editorActiveLineNumber.foreground": "#E4E9E8",
		"editor.selectionBackground": "#00AEC060",
		"editor.inactiveSelectionBackground": "#9DA2A160",
		"editor.selectionHighlightBackground": "#006C7F50",
		"editor.wordHighlightBackground": "#71757550",
		"editor.wordHighlightStrongBackground": "#00829450",
		"editor.findMatchBackground": "#FFB77360",
		"editor.findMatchHighlightBackground": "#FFB77330",
		"editor.findRangeHighlightBackground": "#5C606050",
		"editorLink.activeForeground": "#75F4FF",
		"editorBracketMatch.background": "#5C606050",
		"editorBracketMatch.border": "#5C6060BF",
		"editorError.foreground": "#FF9285",
		"editorWarning.foreground": "#CDB253",
		"editorInfo.foreground": "#67B8FF",
		"editorHint.foreground": "#67B8FF",
		"notification.errorBackground": "#98433B",
		"notification.errorForeground": "#FF9285",
		"notification.infoBackground": "#0060A1",
		"notification.infoForeground": "#67B8FF",
		"notification.warningBackground": "#6D5C12",
		"notification.warningForeground": "#CDB253",
		"debugToolBar.background": "#343838FF",
		"editorWidget.background": "#343838FF",
		"editorSuggestWidget.background": "#101414FF",
		"editorGroup.border": "#474C4B",
		"editorGroupHeader.noTabsBackground": "#101414",
		"editorGroupHeader.tabsBackground": "#101414",
		"tab.border": "#101414",
		"tab.inactiveBackground": "#101414",
		"tab.inactiveForeground": "#9DA2A1",
		"tab.activeBackground": "#101414",
		"tab.activeForeground": "#E4E9E8",
		"tab.hoverBackground": "#222625",
		"tab.hoverBorder": "#008294",
		"tab.activeBorder": "#30C5D7",
		"peekViewEditor.background": "#474C4B55",
		"peekViewTitle.background": "#101414FF",
		"peekView.border": "#474C4B",
		"scrollbar.shadow": "#E4E9E81A",
		"scrollbarSlider.background": "#E4E9E813",
		"scrollbarSlider.activeBackground": "#E4E9E826",
		"scrollbarSlider.hoverBackground": "#E4E9E826",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#006D80",
		"editorGutter.addedBackground": "#136B3C",
		"editorGutter.deletedBackground": "#98433B",
		"editorOverviewRuler.modifiedForeground": "#006D80",
		"editorOverviewRuler.addedForeground": "#136B3C",
		"editorOverviewRuler.deletedForeground": "#98433B",
		"editorOverviewRuler.infoForeground": "#2575BC",
		"editorOverviewRuler.warningForeground": "#847123",
		"editorOverviewRuler.errorForeground": "#B4564D",
		"editorOverviewRuler.wordHighlightForeground": "#717575",
		"editorOverviewRuler.wordHighlightStrongForeground": "#717575",
		"diffEditor.removedTextBackground": "#B4564D26",
		"diffEditor.insertedTextBackground": "#28814D1A",
		"sideBarTitle.foreground": "#E4E9E8",
		"sideBar.background": "#222625",
		"sideBarSectionHeader.background": "#474C4B",
		"list.highlightForeground": "#0098AA",
		"list.hoverBackground": "#474C4B",
		"list.inactiveSelectionBackground": "#7175754D",
		"list.activeSelectionBackground": "#00829440",
		"list.focusBackground": "#00829440",
		"list.inactiveSelectionForeground": "#E4E9E8",
		"list.activeSelectionForeground": "#E4E9E8",
		"list.focusForeground": "#E4E9E8",
		"list.errorForeground": "#FF9285",
		"list.warningForeground": "#CDB253",
		"gitDecoration.modifiedResourceForeground": "#00CBE4",
		"gitDecoration.addedResourceForeground": "#5FC886",
		"gitDecoration.untrackedResourceForeground": "#5FC886",
		"gitDecoration.deletedResourceForeground": "#FF9285",
		"gitDecoration.conflictingResourceForeground": "#FF9285",
		"dropdown.background": "#222625",
		"dropdown.border": "#474C4B",
		"dropdown.foreground": "#B4B9B8",
		"input.background": "#101414",
		"input.border": "#474C4B",
		"input.foreground": "#B4B9B8",
		"input.placeholderForeground": "#717575",
		"button.background": "#0098AA",
		"button.foreground": "#E4E9E8",
		"button.hoverBackground": "#00AEC0",
		"badge.background": "#006C7F",
		"badge.foreground": "#E4E9E8",
		"activityBarBadge.background": "#006C7F",
		"activityBarBadge.foreground": "#E4E9E8",
		"activityBar.background": "#474C4B",
		"activityBar.foreground": "#CCD1D0",
		"statusBar.background": "#343838",
		"statusBar.foreground": "#B4B9B8",
		"statusBar.noFolderBackground": "#343838",
		"statusBar.noFolderForeground": "#B4B9B8",
		"statusBar.debuggingBackground": "#343838",
		"statusBar.debuggingForeground": "#B4B9B8",
		"panel.border": "#474C4B",
		"panelTitle.activeBorder": "#008294",
		"titleBar.activeBackground": "#343838",
		"titleBar.activeForeground": "#CCD1D0",
		"titleBar.inactiveBackground": "#474C4B",
		"titleBar.inactiveForeground": "#B4B9B8",
		"breadcrumb.foreground": "#717575",
		"breadcrumb.focusForeground": "#00AEC0",
		"breadcrumb.activeSelectionForeground": "#00AEC0",
		"breadcrumbPicker.background": "#222625"
	},
	"tokenColors": [
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#B4B9B8"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#B4B9B8"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#717575"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#75F4FF"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#717575"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#E4E9E8"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00AEC0"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#56DCEE"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name.function",
				"entity.name.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#CCD1D0"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E4E9E8"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E4E9E8"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#CCD1D0"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.type",
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#CCD1D0"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#EC7D71"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#EC7D71"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#CCD1D0"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E4E9E8"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#00AEC0"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#CCD1D0"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#CCD1D0"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#CCD1D0"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#E4E9E8"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#CCD1D0"
			}
		}
	]
}