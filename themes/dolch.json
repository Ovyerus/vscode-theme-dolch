{
	"$schema": "vscode://schemas/color-theme",
	"name": "Dolch",
	"colors": {
		"focusBorder": "#008191",
		"foreground": "#B0B5B4",
		"errorForeground": "#BD746A",
		"editor.background": "#101414",
		"editor.foreground": "#B0B5B4",
		"editor.lineHighlightBackground": "#212525",
		"editorCursor.foreground": "#75F4FF",
		"editorLineNumber.foreground": "#6E7372",
		"editorActiveLineNumber.foreground": "#DEE4E3",
		"editor.selectionBackground": "#5ADCED40",
		"editor.selectionHighlightBackground": "#002F3FFF",
		"editor.inactiveSelectionBackground": "#C7CCCB40",
		"editorError.foreground": "#FA9A8E",
		"editorWarning.foreground": "#C9B269",
		"editorInfo.foreground": "#81B7FD",
		"editorHint.foreground": "#81B7FD",
		"notification.errorBackground": "#844F49",
		"notification.errorForeground": "#FA9A8E",
		"notification.infoBackground": "#415F86",
		"notification.infoForeground": "#81B7FD",
		"notification.warningBackground": "#695D34",
		"notification.warningForeground": "#C9B269",
		"debugToolBar.background": "#333737FF",
		"editorWidget.background": "#333737FF",
		"editorSuggestWidget.background": "#101414FF",
		"editorGroup.border": "#464A4A",
		"editorGroupHeader.noTabsBackground": "#101414",
		"editorGroupHeader.tabsBackground": "#333737",
		"tab.border": "#464A4A",
		"tab.inactiveBackground": "#333737",
		"tab.inactiveForeground": "#999E9E",
		"tab.activeBackground": "#101414",
		"tab.activeForeground": "#DEE4E3",
		"peekViewEditor.background": "#464A4A55",
		"peekViewTitle.background": "#101414FF",
		"peekView.border": "#464A4A",
		"scrollbar.shadow": "#DEE4E31A",
		"scrollbarSlider.background": "#DEE4E313",
		"scrollbarSlider.activeBackground": "#DEE4E326",
		"scrollbarSlider.hoverBackground": "#DEE4E326",
		"editorOverviewRuler.border": "#00000000",
		"editorGutter.modifiedBackground": "#00525B",
		"editorGutter.addedBackground": "#2D5139",
		"editorGutter.deletedBackground": "#693E39",
		"editorOverviewRuler.modifiedForeground": "#00525B",
		"editorOverviewRuler.addedForeground": "#2D5139",
		"editorOverviewRuler.deletedForeground": "#693E39",
		"editorOverviewRuler.infoForeground": "#5174A3",
		"editorOverviewRuler.warningForeground": "#807140",
		"editorOverviewRuler.errorForeground": "#A06159",
		"diffEditor.removedTextBackground": "#A0615926",
		"diffEditor.insertedTextBackground": "#497D5A1A",
		"sideBarTitle.foreground": "#DEE4E3",
		"sideBar.background": "#212525",
		"sideBarSectionHeader.background": "#464A4A",
		"list.highlightForeground": "#0097A7",
		"list.hoverBackground": "#464A4A",
		"list.inactiveSelectionBackground": "#6E73724D",
		"list.activeSelectionBackground": "#00819140",
		"list.focusBackground": "#00819140",
		"list.inactiveSelectionForeground": "#DEE4E3",
		"list.activeSelectionForeground": "#DEE4E3",
		"list.focusForeground": "#DEE4E3",
		"list.errorForeground": "#FA9A8E",
		"list.warningForeground": "#C9B269",
		"gitDecoration.modifiedResourceForeground": "#00C6DA",
		"gitDecoration.addedResourceForeground": "#76C490",
		"gitDecoration.untrackedResourceForeground": "#76C490",
		"gitDecoration.deletedResourceForeground": "#FA9A8E",
		"gitDecoration.conflictingResourceForeground": "#FA9A8E",
		"dropdown.background": "#333737",
		"dropdown.border": "#464A4A",
		"dropdown.foreground": "#B0B5B4",
		"input.background": "#101414",
		"input.border": "#464A4A",
		"input.foreground": "#B0B5B4",
		"input.placeholderForeground": "#6E7372",
		"button.background": "#0097A7",
		"button.foreground": "#DEE4E3",
		"button.hoverBackground": "#04ADBE",
		"badge.background": "#006B7C",
		"badge.foreground": "#DEE4E3",
		"activityBarBadge.background": "#006B7C",
		"activityBarBadge.foreground": "#DEE4E3",
		"activityBar.background": "#464A4A",
		"activityBar.foreground": "#C7CCCB",
		"statusBar.background": "#333737",
		"statusBar.foreground": "#B0B5B4",
		"statusBar.noFolderBackground": "#333737",
		"statusBar.noFolderForeground": "#B0B5B4",
		"statusBar.debuggingBackground": "#333737",
		"statusBar.debuggingForeground": "#B0B5B4",
		"panel.border": "#464A4A",
		"panelTitle.activeBorder": "#008191",
		"titleBar.activeBackground": "#333737",
		"titleBar.activeForeground": "#C7CCCB",
		"titleBar.inactiveBackground": "#464A4A",
		"titleBar.inactiveForeground": "#B0B5B4",
		"breadcrumb.foreground": "#838888",
		"breadcrumb.focusForeground": "#04ADBE",
		"breadcrumb.activeSelectionForeground": "#04ADBE",
		"breadcrumbPicker.background": "#212525"
	},
	"tokenColors": [
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#B0B5B4"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#B0B5B4"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#838888"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#75F4FF"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"delimiter",
				"bracket",
				"brace",
				"paren",
				"delimiter.tag",
				"punctuation.tag",
				"tag.html",
				"tag.xml",
				"meta.property-value punctuation.separator.key-value",
				"punctuation.definition.metadata.md",
				"string.link.md",
				"meta.brace"
			],
			"settings": {
				"foreground": "#6E7372"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#DEE4E3"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"meta.property-value.string",
				"support.constant.property-value.string",
				"meta.structure.dictionary.value.json string.quoted.double.json",
				"meta.structure.dictionary.json string.quoted.double.json",
				"meta.preprocessor string"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#04ADBE"
			}
		},
		{
			"name": "Primitive Literals",
			"scope": [
				"constant.numeric",
				"meta.property-value.numeric",
				"support.constant.property-value.numeric",
				"meta.property-value.color",
				"support.constant.property-value.color",
				"constant.language"
			],
			"settings": {
				"foreground": "#5ADCED"
			}
		},
		{
			"name": "User names",
			"scope": [
				"constant.character",
				"constant.other",
				"entity.name.function",
				"entity.name.class",
				"entity.other.inherited-class",
				"entity.other.attribute-name",
				"entity.name",
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"support.type.property-name",
				"entity.name.tag.table",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "#C7CCCB"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"meta.property-value.keyword",
				"support.constant.property-value.keyword",
				"meta.preprocessor.keyword",
				"keyword.other.use",
				"keyword.other.function.use",
				"keyword.other.namespace",
				"keyword.other.new",
				"keyword.other.special-method",
				"keyword.other.unit",
				"keyword.other.use-as"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DEE4E3"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type",
				"storage.type.ts",
				"storage.type.var.ts",
				"storage.type.js",
				"storage.type.var.js",
				"storage.type.const.ts",
				"storage.type.let.ts",
				"storage.type.let.js",
				"storage.type.const.js",
				"entity.name.tag"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DEE4E3"
			}
		},
		{
			"name": "Pointer, access, etc",
			"scope": [
				"meta.ptr",
				"meta.pointer",
				"meta.address",
				"meta.array.cxx"
			],
			"settings": {}
		},
		{
			"name": "Preprocessor",
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#C7CCCB"
			}
		},
		{
			"name": "Library",
			"scope": [
				"support.type",
				"support.class",
				"support.function",
				"support.constant"
			],
			"settings": {
				"foreground": "#C7CCCB"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#DB877C"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"foreground": "#DB877C"
			}
		},
		{
			"name": "Markdown Title Hash",
			"scope": [
				"punctuation.definition.heading.md",
				"entity.name.type.md",
				"beginning.punctuation"
			],
			"settings": {
				"foreground": "#C7CCCB"
			}
		},
		{
			"name": "Markdown titles",
			"scope": [
				"markup.heading",
				"entity.name.section"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DEE4E3"
			}
		},
		{
			"name": "Markdown Raw",
			"scope": [
				"markup.raw",
				"markup.inline.raw",
				"markup.fenced",
				"markup.fenced_code"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#04ADBE"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.link",
				"string.other.link.title",
				"string.other.link.description",
				"meta.link.inline",
				"meta.image.inline"
			],
			"settings": {
				"foreground": "#C7CCCB"
			}
		},
		{
			"name": "Makefile Variables",
			"scope": [
				"variable.language.makefile",
				"variable.other.makefile"
			],
			"settings": {
				"foreground": "#C7CCCB"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "CSS Class",
			"scope": [
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#C7CCCB"
			}
		},
		{
			"name": "CSS Tag name",
			"scope": [
				"entity.name.tag.css"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DEE4E3"
			}
		},
		{
			"name": "CSS Property",
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#C7CCCB"
			}
		}
	]
}