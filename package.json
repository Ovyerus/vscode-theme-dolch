{
	"name": "theme-dolch",
	"version": "0.3.0",
	"engineStrict": true,
	"engines": {
		"vscode": "^1.33.0"
	},
	"devDependencies": {
		"@types/node": "~8.5.10",
		"rimraf": "~2.6.2",
		"tslint": "^5.10.0",
		"typescript": "^3.0.0",
		"prettier": "^2.0.1"
	},
	"scripts": {
		"clean": "rimraf coverage lib tmp",
		"build": "tsc -p tsconfig.json && node lib/index",
		"watch": "tsc -w -p tsconfig.json"
	},
	"author": "Renzhi Li aka Belleve Invis<belleve@typeof.net>",
	"repository": {
		"type": "git",
		"url": "https://github.com/be5invis/vscode-theme-dolch"
	},
	"license": "MIT",
	"dependencies": {
		"tslib": "~1.9.0"
	},
	"publisher": "be5invis",
	"displayName": "Dolch Theme",
	"description": "Dolch themes for VS Code.",
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Dolch",
				"uiTheme": "vs-dark",
				"path": "./themes/dolch.json"
			},
			{
				"label": "Dolch Light",
				"uiTheme": "vs",
				"path": "./themes/dolch-light.json"
			}
		],
		"iconThemes": [
			{
				"id": "dolch",
				"label": "Dolch",
				"path": "./icons/theme.json"
			}
		]
  }
}
